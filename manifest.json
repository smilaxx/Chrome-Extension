{
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [ "https://trello.com/*" ],
            "css": [ "trellowip.css" ],
            "js": [ "jquery-1.7.1.min.js", "trellowip.js" ]
        }
    ],
    "description": "Adds work-in-progress limits to Trello lists supporting a Kanban workflow.",
    "icons": {
        "128": "icons/icon128.png"
    },
    "name": "Pronama WIPTrello",
    "version": "0.0"
}